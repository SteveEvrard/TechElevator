 isBlindTasting: false\n      },\n      whiskey: {\n        brand: String,\n        whiskeyId: Number\n      },\n      apiURLEvent: \"http://localhost:8080/AuthenticationApplication/api/events\",\n      API_URL: \"http://localhost:8080/AuthenticationApplication/api/whiskeys\",\n      whiskeyRating: {\n        whiskeyId: Number,\n        userId: Number,\n        eventId: Number,\n        tasteRating: 0,\n        smellRating: 0,\n        colorRating: 0,\n        bodyRating: 0,\n        finishRating: 0,\n        priceRating: 0,\n        overallRating: 0\n      },\n      slider: {\n        value: 3,\n        draggable: true,\n        processStyle: {\n          backgroundColor: \"#2E4D58\"\n        }\n      }\n    };\n  },\n  created() {\n    this.updateWhiskeyRating(event);\n  },\n  methods: {\n    saveWhiskeyRating() {\n      fetch(this.apiURLEvent, {\n        method: \"POST\",\n        headers: {\n          \"Content-Type\": \"application/json\"\n        },\n        body: JSON.stringify(this.eventData)\n      })\n        .then(response => {\n          if (response.ok) {\n            this.$emit(\"showReviews\");\n            this.$router.push(\"/\");\n          }\n        })\n        .catch(err => console.error(err));\n    }\n  },\n  rateTaste(value) {},\n  updateWhiskeyRating(event) {\n    this.whiskeyRating.eventId = this.event.eventId;\n  },\n  isFinished() {\n    this.$emit(\"finished\");\n  }\n};\n",{"version":3,"sources":["RateSingleWhiskey.vue"],"names":[],"mappings":";;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;AA6EA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA","file":"RateSingleWhiskey.vue","sourceRoot":"src/components/Whiskey","sourcesContent":["<template>\n  <form-format>\n    <tile-format>\n      <h2 v-if=\"!event.isBlindTasting\">{{whiskey.brand}}</h2>\n\n      <h4>\n        Rate the\n        <h4 class=\"emphasis-word\">taste</h4>of the whiskey:\n      </h4>\n      <VueSlideBar\n        class=\"slider\"\n        v-model=\"slider.value\"\n        :processStyle=\"slider.processStyle\"\n        :labelStyles=\"{ color: 'white', backgroundColor: 'transparent' }\"\n        :min=\"1\"\n        :max=\"5\"\n        :lineHeight=\"20\"\n        :tooltipStyles=\"{ backgroundColor: '#2E4D58', borderColor: '#2E4D58' }\"\n        :draggable=\"true\"\n      >></VueSlideBar>\n      <rating-buttons-1-thru-5 v-bind:variableData=\"whiskeyRating.tasteRating\"></rating-buttons-1-thru-5>\n\n      <h4>\n        Rate the\n        <h4 class=\"emphasis-word\">smell</h4>of the whiskey:\n      </h4>\n      <rating-buttons-1-thru-5\n        v-bind:variableData=\"whiskeyRating.smellRating\"\n        v-on:israted=\"whiskeyRating.smellRating=rating\"\n      ></rating-buttons-1-thru-5>\n\n      <h4>\n        Rate the\n        <h4 class=\"emphasis-word\">color</h4>of the whiskey:\n      </h4>\n      <rating-buttons-1-thru-5\n        v-bind:variableData=\"whiskeyRating.colorRating\"\n        v-on:israted=\"whiskeyRating.colorRating=rating\"\n      ></rating-buttons-1-thru-5>\n\n      <h4>\n        Rate the\n        <h4 class=\"emphasis-word\">body</h4>of the whiskey:\n      </h4>\n      <rating-buttons-1-thru-5\n        v-bind:variableData=\"whiskeyRating.bodyRating\"\n        v-on:israted=\"whiskeyRating.bodyRating=rating\"\n      ></rating-buttons-1-thru-5>\n\n      <h4>\n        Rate the\n        <h4 class=\"emphasis-word\">finish</h4>of the whiskey:\n      </h4>\n      <rating-buttons-1-thru-5\n        v-bind:variableData=\"whiskeyRating.finishRating\"\n        v-on:israted=\"whiskeyRating.finishRating=rating\"\n      ></rating-buttons-1-thru-5>\n\n      <h4>\n        Rate the\n        <h4 class=\"emphasis-word\">price</h4>of the whiskey:\n      </h4>\n      <rating-buttons-1-thru-5\n        v-bind:variableData=\"whiskeyRating.priceRating\"\n        v-on:israted=\"whiskeyRating.priceRating=rating\"\n      ></rating-buttons-1-thru-5>\n\n      <h4 class=\"emphasis-word\">What did you think of the whiskey overall?</h4>\n      <rating-buttons-1-thru-5\n        v-bind:variableData=\"whiskeyRating.overallRating\"\n        v-on:israted=\"whiskeyRating.overallRating=rating, saveWhiskeyRating(), isFinished()\"\n      ></rating-buttons-1-thru-5>\n    </tile-format>\n  </form-format>\n</template>\n\n<script>\nimport TileFormat from \"@/components/Formatting/TileFormat.vue\";\nimport FormFormat from \"@/components/Formatting/FormFormat.vue\";\nimport VueSlideBar from \"vue-slide-bar\";\n\nexport default {\n  components: {\n    TileFormat,\n    FormFormat,\n    VueSlideBar\n  },\n  name: \"RateSingleWhiskey\",\n  prop: {\n    event: {\n      title: \"\",\n      // eventImageURL: \"\",\n      isPrivate: true,\n      date: new Date(),\n      time: \"\",\n      location: \"\",\n      eventDescription: \"\",\n      isBlindTasting: false\n    }\n  },\n  data() {\n    return {\n      event: {\n        isBlindTasting: false\n      },\n      whiskey: {\n        brand: String,\n        whiskeyId: Number\n      },\n      apiURLEvent: \"http://localhost:8080/AuthenticationApplication/api/events\",\n      API_URL: \"http://localhost:8080/AuthenticationApplication/api/whiskeys\",\n      whiskeyRating: {\n        whiskeyId: Number,\n        userId: Number,\n        eventId: Number,\n        tasteRating: 0,\n        smellRating: 0,\n        colorRating: 0,\n        bodyRating: 0,\n        finishRating: 0,\n        priceRating: 0,\n        overallRating: 0\n      },\n      slider: {\n        value: 3,\n        draggable: true,\n        processStyle: {\n          backgroundColor: \"#2E4D58\"\n        }\n      }\n    };\n  },\n  created() {\n    this.updateWhiskeyRating(event);\n  },\n  methods: {\n    saveWhiskeyRating() {\n      fetch(this.apiURLEvent, {\n        method: \"POST\",\n        headers: {\n          \"Content-Type\": \"application/json\"\n        },\n        body: JSON.stringify(this.eventData)\n      })\n        .then(response => {\n          if (response.ok) {\n            this.$emit(\"showReviews\");\n            this.$router.push(\"/\");\n          }\n        })\n        .catch(err => console.error(err));\n    }\n  },\n  rateTaste(value) {},\n  updateWhiskeyRating(event) {\n    this.whiskeyRating.eventId = this.event.eventId;\n  },\n  isFinished() {\n    this.$emit(\"finished\");\n  }\n};\n</script>\n\n<style>\n.create {\n  text-align: center;\n  align-items: center;\n  align-self: center;\n}\n\nh4 {\n  display: inline-block;\n  margin: 20px 0px 10px 0px;\n}\n\n#rating-choice {\n  background: #2e4d58;\n  color: white;\n  padding: 5px 10px 5px 10px;\n  border-radius: 0px;\n  font-size: 1.1em;\n  margin: 0px 5px 0px 5px;\n  border-radius: 3px;\n}\n\n#rating-choice:hover {\n  background: #527785;\n  border: 2px solid #2e4d58;\n  color: white;\n}\n\n#rating-choice::selection {\n  background: white;\n  color: #2e4d58;\n}\n\n.emphasis-word {\n  color: #9d432c;\n  padding-right: 5px;\n  padding-left: 2px;\n  font-size: 1.1em;\n}\n</style>\n\n\n\n\n"]}]}