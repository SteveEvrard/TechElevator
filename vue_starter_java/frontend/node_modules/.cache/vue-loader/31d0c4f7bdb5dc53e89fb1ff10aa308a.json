  isLoggedIn: true,\n      hasRated: false,\n      whiskeyRating: {\n        whiskey: {\n          brand: \"\",\n          price: 0\n        },\n        whiskeyId: 0,\n        userId: 0,\n        eventId: 0,\n        tasteRating: 0,\n        smellRating: 0,\n        colorRating: 0,\n        bodyRating: 0,\n        finishRating: 0,\n        priceRating: 0,\n        overallRating: 0\n      },\n      whiskeyRatingList: [],\n      eventId: 0,\n      thisEvent: this.event\n    };\n  },\n  name: \"SingleEvent\",\n  methods: {\n    showdetail(event) {\n      let eventId = this.event.eventId;\n      this.$router.push({ name: \"eventDetailPage\", params: { eventId } });\n    }\n  }\n};\n</script>\n\n<style>\n.single-event h4 {\n  margin: 10px 0px 5px 0px;\n  font-size: 1.5em;\n  font-weight: 500px;\n  padding-right: 10px;\n  display: inline-block;\n  color: black;\n}\n.single-event p {\n  margin: 0px;\n  display: inline-block;\n  font-size: 1.2em;\n}\n.single-event h2 {\n  text-align: center;\n  margin: 10px;\n  font-family: \"Great Vibes\", sans-serif;\n  font-size: 2.5em;\n  font-weight: 300px;\n}\n.single-event {\n  padding: 5px;\n  background-color: white;\n  width: 350px;\n  height: 250px;\n}\n\n#line-div {\n  width: 100%;\n  display: block;\n  padding: 5px 20px 5px 20px;\n}\n</style>"]}]}