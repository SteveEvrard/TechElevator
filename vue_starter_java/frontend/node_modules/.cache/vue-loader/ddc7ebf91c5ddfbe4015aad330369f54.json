ton>\n    </h4>\n  </div>\n</template>\n\n<script>\nimport RateSingleWhiskey from \"@/components/Whiskey/RateSingleWhiskey.vue\";\n\nexport default {\n  components: {\n    RateSingleWhiskey\n  },\n  prop: {\n    eventData: {\n      title: String,\n      // eventImageURL: \"\",\n      eventId: Number,\n      title: String,\n      imgUrl: String,\n      date: new Date(),\n      time: String,\n      location: String,\n      info: String,\n      tastingWhiskeys: Array\n    },\n    whiskey: {\n      brand: String,\n      whiskeyId: Number\n    }\n  },\n  data() {\n    return {\n      API_URL: \"http://localhost:8080/AuthenticationApplication/api/whiskeys\"\n    };\n  },\n  methods: {\n    saveWhiskey() {\n      fetch(this.apiURLEvent, {\n        method: \"POST\",\n        headers: {\n          \"Content-Type\": \"application/json\"\n        },\n        body: JSON.stringify(this.eventData)\n      })\n        .then(response => {\n          if (response.ok) {\n            this.$emit(\"showReviews\");\n            this.$router.push(\"/\");\n          }\n        })\n        .catch(err => console.error(err));\n    }\n  }\n};\n</script>\n\n<style>\n.create {\n  text-align: center;\n  align-items: center;\n  align-self: center;\n}\n</style>\n\n\n\n\n"]}]}