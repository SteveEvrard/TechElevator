l</th>\n          </tr>\n          <tr v-for=\"rating in whiskeyRatingList\" :key=\"rating\">\n            <td>{{rating.whiskey.brand}}</td>\n            <td>{{rating.tasteRating}}</td>\n            <td>{{rating.smellRating}}</td>\n            <td>{{rating.colorRating}}</td>\n            <td>{{rating.finishRating}}</td>\n            <td>{{rating.priceRating}}</td>\n            <td>{{rating.overallRating}}</td>\n          </tr>\n        </table>\n      </div>\n    </div>\n  </tile-format>\n</template>\n\n<script>\nimport TileFormat from \"@/components/Formatting/TileFormat.vue\";\n\nexport default {\n  components: {\n    TileFormat\n  },\n  props: {\n    isHOME: Boolean,\n    event: {\n      eventId: Number,\n      title: String,\n      // imgUrl: String,\n      date: new Date(),\n      time: String,\n      location: String,\n      eventDescription: String,\n      isPrivate: Boolean,\n      isBlindTasting: Boolean\n    }\n  },\n  data() {\n    return {\n      isHomePage: Boolean(this.isHOME),\n      isLoggedIn: true,\n      hasRated: false,\n      whiskeyRating: {\n        whiskey: {\n          brand: \"\",\n          price: 0\n        },\n        whiskeyId: 0,\n        userId: 0,\n        eventId: 0,\n        tasteRating: 0,\n        smellRating: 0,\n        colorRating: 0,\n        bodyRating: 0,\n        finishRating: 0,\n        priceRating: 0,\n        overallRating: 0\n      },\n      whiskeyRatingList: [],\n      eventId: 0,\n      thisEvent: this.event\n    };\n  },\n  name: \"SingleEvent\",\n  created() {\n    this.eventId = event.eventId;\n    console.log(this.eventId);\n  },\n  methods: {\n    showdetail(event) {\n      let eventId = this.event.eventId;\n      this.$router.push({ name: \"eventDetailPage\", params: { eventId } });\n    }\n  }\n};\n</script>\n\n<style>\n</style>"]}]}