ation/api/whiskeys\",\n      eventData: {\n        title: \"\",\n        // eventImageURL: \"\",\n        isPrivate: true,\n        date: new Date(),\n        time: \"\",\n        location: \"\",\n        eventDescription: \"\",\n        isBlindTasting: true\n      }\n    };\n  },\n  methods: {\n    saveEvent: function saveEvent() {\n      var _this = this;\n\n      fetch(this.apiURLEvent, {\n        method: \"POST\",\n        headers: {\n          \"Content-Type\": \"application/json\"\n        },\n        body: JSON.stringify(this.eventData)\n      }).then(function (response) {\n        if (response.ok) {\n          var eventId = _this.event.eventId;\n\n          _this.$router.push({\n            name: \"eventPage\",\n            params: {\n              eventId: eventId\n            }\n          });\n        }\n      }).catch(function (err) {\n        return console.error(err);\n      });\n    }\n  }\n};",{"version":3,"sources":["CreateEvent.vue"],"names":[],"mappings":";;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;AAsDA,OAAA,UAAA,MAAA,wCAAA;AACA,OAAA,qBAAA,MAAA,gDAAA;AAEA,eAAA;AACA,EAAA,UAAA,EAAA;AACA,IAAA,UAAA,EAAA,UADA;AAEA,IAAA,qBAAA,EAAA;AAFA,GADA;AAMA,EAAA,IANA,kBAMA;AACA,WAAA;AACA,MAAA,WAAA,EAAA,4DADA;AAEA,MAAA,OAAA,EAAA,8DAFA;AAGA,MAAA,SAAA,EAAA;AACA,QAAA,KAAA,EAAA,EADA;AAEA;AACA,QAAA,SAAA,EAAA,IAHA;AAIA,QAAA,IAAA,EAAA,IAAA,IAAA,EAJA;AAKA,QAAA,IAAA,EAAA,EALA;AAMA,QAAA,QAAA,EAAA,EANA;AAOA,QAAA,gBAAA,EAAA,EAPA;AAQA,QAAA,cAAA,EAAA;AARA;AAHA,KAAA;AAcA,GArBA;AAsBA,EAAA,OAAA,EAAA;AACA,IAAA,SADA,uBACA;AAAA;;AACA,MAAA,KAAA,CAAA,KAAA,WAAA,EAAA;AACA,QAAA,MAAA,EAAA,MADA;AAEA,QAAA,OAAA,EAAA;AACA,0BAAA;AADA,SAFA;AAKA,QAAA,IAAA,EAAA,IAAA,CAAA,SAAA,CAAA,KAAA,SAAA;AALA,OAAA,CAAA,CAOA,IAPA,CAOA,UAAA,QAAA,EAAA;AACA,YAAA,QAAA,CAAA,EAAA,EAAA;AACA,cAAA,OAAA,GAAA,KAAA,CAAA,KAAA,CAAA,OAAA;;AACA,UAAA,KAAA,CAAA,OAAA,CAAA,IAAA,CAAA;AAAA,YAAA,IAAA,EAAA,WAAA;AAAA,YAAA,MAAA,EAAA;AAAA,cAAA,OAAA,EAAA;AAAA;AAAA,WAAA;AACA;AACA,OAZA,EAaA,KAbA,CAaA,UAAA,GAAA;AAAA,eAAA,OAAA,CAAA,KAAA,CAAA,GAAA,CAAA;AAAA,OAbA;AAcA;AAhBA;AAtBA,CAAA","sourcesContent":["<template>\n  <div id=\"create-event\">\n    <form-format class=\"event-background\">\n      <h1>Create a New Event</h1>\n\n      <h4>Title:</h4>\n      <br>\n      <input type=\"text\" v-model=\"eventData.title\">\n      <br>\n\n      <h4>\n        Is this a private event?\n        <input type=\"checkbox\" v-model=\"eventData.isPrivate\">\n        {{eventData.isPrivate ? \"yes\" : \"no\"}}\n      </h4>\n      <br>\n      <h4>Date of Event:</h4>\n      <br>\n      <input type=\"date\" v-model=\"eventData.date\">\n      <br>\n\n      <h4>Time of Event:</h4>\n      <br>\n      <input type=\"text\" v-model=\"eventData.time\">\n      <br>\n\n      <h4>Location:</h4>\n      <br>\n      <input type=\"text\" v-model=\"eventData.location\">\n      <br>\n\n      <h4>Additional Information:</h4>\n      <br>\n      <textarea v-model=\"eventData.eventDescription\"></textarea>\n      <br>\n\n      <h4>Is this a blind tasting?\n      <input type=\"checkbox\" v-model=\"eventData.isBlindTasting\">\n      {{eventData.isBlindTasting ? \"yes\" : \"no\"}}\n      </h4>\n      <br>\n      <h4>What whiskeys will be tasted?</h4>\n      <h4>\n        <whiskey-brands-to-select :apiURL=\"API_URL\"></whiskey-brands-to-select>\n      </h4>\n      <br>\n      <h4>\n        <button type=\"submit\" v-on:click.prevent=\"saveEvent\">Create Event</button>\n      </h4>\n    </form-format>\n  </div>\n</template>\n\n<script>\nimport FormFormat from \"@/components/Formatting/FormFormat.vue\";\nimport WhiskeyBrandsToSelect from \"@/components/Whiskey/WhiskeyBrandsToSelect.vue\";\n\nexport default {\n  components: {\n    FormFormat,\n    WhiskeyBrandsToSelect\n  },\n\n  data() {\n    return {\n      apiURLEvent: \"http://localhost:8080/AuthenticationApplication/api/events\",\n      API_URL: \"http://localhost:8080/AuthenticationApplication/api/whiskeys\",\n      eventData: {\n        title: \"\",\n        // eventImageURL: \"\",\n        isPrivate: true,\n        date: new Date(),\n        time: \"\",\n        location: \"\",\n        eventDescription: \"\",\n        isBlindTasting: true\n      }\n    };\n  },\n  methods: {\n    saveEvent() {\n      fetch(this.apiURLEvent, {\n        method: \"POST\",\n        headers: {\n          \"Content-Type\": \"application/json\"\n        },\n        body: JSON.stringify(this.eventData)\n      })\n        .then(response => {\n          if (response.ok) {\n            let eventId = this.event.eventId;\n            this.$router.push({ name: \"eventPage\", params: { eventId } });\n          }\n        })\n        .catch(err => console.error(err));\n    }\n  }\n};\n</script>\n\n<style>\n.create {\n  text-align: center;\n  align-items: center;\n  align-self: center;\n}\n\n.event-background {\n  background: #f1f1f1;\n  margin: 0px;\n  border: 3px solid #f1f1f1;\n  width: 50%;\n  align-content: center;\n}\n\n#create-event {\n  display: flex;\n  justify-content: center;\n}\n\n/* Full-width inputs */\n.event-background > input[type=text], textarea, input{\n  width: 80%;\n  padding: 12px 20px;\n  margin: 8px 0;\n  display: inline-block;\n  border: 1px solid #ccc;\n  box-sizing: border-box;\n}\n\n/* Set a style for all buttons */\nbutton {\n  background-color: rgb(44, 82, 52);\n  color: white;\n  padding: 14px 20px;\n  margin: 8px 0;\n  border: none;\n  cursor: pointer;\n  width: 100%;\n}\n\n/* Add a hover effect for buttons */\nbutton:hover {\n  opacity: 0.8;\n}\n\n</style>\n\n\n\n\n"],"sourceRoot":"src/views"}]}